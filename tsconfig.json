{
    "compilerOptions": {
      "jsx": "react-jsx",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./dist/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5",
      "allowJs": true,
      "esModuleInterop": true,
      "strictNullChecks":false
    },
    "include": [
      "src/**/*",
    ]
  }